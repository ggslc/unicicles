KG_DIR=kg_ioput
TEST_DIR=output
ISCRIPDIR=LINK_TO_REFACTOR_SCRIPTS
LDLIBPATH=LINK_TO_LDLIBRARY

export PYTHONPATH=$PYTHONPATH:$ISCRIPDIR
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$LDLIBPATH

PREICE_NEMO_IN=$KG_DIR/nemo_cu858c_P1Y_20440101-20450101_icecouple
PREICE_NEMO_OUT=$TEST_DIR/nemo_cu858c_P1Y_20440101-20450101_icecouple

python $ISCRIPDIR/nemo4.2_to_regional_bisicles.py --input ${PREICE_NEMO_IN}.nc --output ${PREICE_NEMO_OUT}-AIS.hdf5 --region AIS
